		</alternativa>
		
		<alternativa> 
			<idAlt> ad009b </idAlt>
			<codigo> malloc(V)</codigo>	
		</alternativa>
		
		<alternativa> 
			<idAlt> ad009c </idAlt>
			<codigo> sizeof(V)</codigo>	
		</alternativa>
		
		<alternativa> 
			<idAlt> ad009d </idAlt>
			<codigo> free(V)</codigo>	
