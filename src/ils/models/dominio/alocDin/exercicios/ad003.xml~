		<alternativa> 
			<idAlt> ad003a </idAlt>
			<codigo> vet = (int*) malloc (sizeof(int)) </codigo>
		</alternativa>
		
		<alternativa> 
			<idAlt> ad003b </idAlt>
			<codigo> *vet = (int*) malloc (sizeof(int)) </codigo>	
		</alternativa>
		
		<alternativa> 
			<idAlt> ad003c </idAlt>
