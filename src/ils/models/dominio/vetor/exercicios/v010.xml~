		<enunciado> Dada uma sequência de n números, imprime-las na ordem inversa à da leitura. </enunciado>

		<alternativa> 
			<idAlt> v010a </idAlt>
			<codigo>
				for (i = 0; i &lt; n; i++) 
					scanf(&quot;%d&quot;, &amp;v[i]);
						for (i = n-1; i &gt;= 0; i--)
							printf(&quot;%d&quot;, v[i]); 
			</codigo>
		</alternativa>
		
		<alternativa> 
			<idAlt> v010b </idAlt>
			<codigo>
				for (i = 1; i &lt; n; i++)
					scanf(&quot;%d&quot;, &amp;v[i]);
						for (i = n-1; i &gt;= 1; i--)
							printf(&quot;%d&quot;, v[i]);
			</codigo>	
		</alternativa>
		
		<alternativa> 
			<idAlt> v010c </idAlt>
			<codigo> 
				for (i = 1; i &lt; n; i++)
					scanf(&quot;%d&quot;, &amp;v[i]);
						for (i = n; i &gt;= 1; i--)
							printf(&quot;%d&quot;, v[i]);
			</codigo>	
		</alternativa>
		
		<alternativa> 
			<idAlt> v010d </idAlt>
			<codigo> 
				for (i = 1; i &lt; n; i++)
					scanf(&quot;%d&quot;, &amp;v[i]);
						for (i = n; i &gt; 1; i--)
							printf(&quot;%d&quot;, v[i]);
			</codigo>	
		</alternativa>
		

